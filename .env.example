# OpenAI API Key (required for GPT models)
OPENAI_API_KEY=sk-your-openai-key-here

# Anthropic API Key (required for Claude models)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Google Gemini API Key
GEMINI_API_KEY=your-gemini-key-here

# DeepSeek API Key
DEEPSEEK_API_KEY=your-deepseek-key-here

# LiteLLM Master Key (generate a random string)
LITELLM_MASTER_KEY=sk-your-litellm-master-key

# Database Credentials
POSTGRES_USER=litellmadmin
POSTGRES_PASSWORD=change-this-password
POSTGRES_DB=litellm_db

# OpenWebUI Secret Key (generate with: openssl rand -hex 32)
WEBUI_SECRET_KEY=your-random-secret-key
